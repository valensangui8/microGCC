
int main(){
    int a = 5;
    int b = 10;
    int c = 15;
    int result1;
    int result2;
    int result3;
    int result4;
    
    /* Logical operations */
    result1 = (a < b) && (b < c);  /* Logical AND */
    result2 = (a > b) || (b < c);  /* Logical OR */
    result3 = !(a > b);            /* Logical NOT */
    result4 = (a < b) && !(b > c); /* Combination of operators */
    
    return result4;
}